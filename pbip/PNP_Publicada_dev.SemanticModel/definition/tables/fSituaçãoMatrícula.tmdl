	/// @description Matrícula atendida
	/// @author dsbr
	/// @version 1
	/// @date 03/11/2022
	measure 'Matrícula atendida' =
			
			/*
			@description Matrícula atendida
			@author dsbr
			@version 1
			@date 03/11/2022
			*/
			
			VAR ano =
			    VALUE ( MAX ( 'dCalendário'[Ano] ) )
			RETURN
			CALCULATE (
			                [Número de Matrículas],
			                YEAR ( 'fSituaçãoMatrícula'[dataMatricula] ) <= ano,
			                dimSituacao[nomeSituacao] = "Em curso"
			            )
			                + CALCULATE (
			                    [Número de Matrículas],
			                    YEAR ( 'fSituaçãoMatrícula'[dataMatricula] ) <= ano,
			                    dimSituacao[nomeSituacao] <> "Em curso",
			                    YEAR ( 'fSituaçãoMatrícula'[dataSituacao] ) >= ano
			                )
		formatString: #,0
		lineageTag: 2cc10cf3-8c4a-4406-84f3-22021209c015

		changedProperty = FormatString

	/// @description Corresponde à matrícula ponderada pelos fatores de: - Fator de Equiparação de carga horária; - Fator de Esforço de curso.
	/// @author dsbr
	/// @version 1
	/// @date 03/11/2022
	measure 'Matrícula Equivalente Apresentada Geral' =
			
			/*
			@description Corresponde à matrícula ponderada pelos fatores de: - Fator de Equiparação de carga horária; - Fator de Esforço de curso.
			@author dsbr
			@version 1
			@date 03/11/2022
			*/
			
			VAR ano =
			    VALUE ( MAX ( 'dCalendário'[Ano] ) )
			VAR tabela =
			    SUMMARIZE (
			        dimCurso,
			        dimCurso[FECH],
			        dimCurso[fatorEsforcoCurso],
			        "matr_atend",'fSituaçãoMatrícula'[Matrícula atendida]
			    )
			RETURN
			    SUMX ( tabela, [FECH] * [fatorEsforcoCurso] * [matr_atend] )
		formatString: #,0
		lineageTag: e9d00777-3258-4c7a-b47a-00a587580725

		changedProperty = FormatString

